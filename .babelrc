{
  "presets": [
    "es2015",
    "react",
    "stage-0",
    "next/babel"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ],
    [
      "css-modules-transform", {
        "extensions": [".css", ".scss"],
        "append": [ "postcss-cssnext" ],
        "extractCss": "./static/css/bundle.css",
        "preprocessCss": "./pre-processor.js"
      }
    ]
  ]
}
